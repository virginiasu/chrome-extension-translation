{
    "manifest_version": 3,
    "name": "Realtime Translation on Web",
    "author": "Yuan Su",
    "description": "Translate the selected text on the web page in real time.",
    "version": "1.1.0",
    "permissions": [
        "activeTab",
        "scripting",
        "contextMenus"
    ],
    "background": {
        "service_worker": "index.js"
    },
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    }
}